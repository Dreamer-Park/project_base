{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "DJANGO:RUNSERVER",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "pythonPath": "${workspaceFolder}/venv/bin/python",
            "args": [
                "runserver",
                "--settings",
                "main.settings.developenv"

            ],
            "django": true
        },
        {
            "name": "DJANGO:SHELL",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "pythonPath": "${workspaceFolder}/venv/bin/python",
            "args": [
                "shell_plus", 
            ],
            "django": true
        }
    ]
}